	"version": "0.2.0",
	"configurations": [
		{
			"command": "npm start",
			"name": "Run npm start",
			"request": "launch",
			"type": "node-terminal"
		},
		{
			"type": "pwa-msedge",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:5000",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "iOS Web",
			"type": "pwa-msedge",
			"request": "attach",
			"port": 9000,
			"url": "http://localhost:8888/*",
			"webRoot": "${workspaceRoot}/src"
		}
	]
}
